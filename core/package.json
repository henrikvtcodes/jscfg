{
  "name": "jscfg",
  "version": "0.0.1",
  "description": "All-in-one configuration source for JavaScript/TypeScript projects",
  "author": "Henrik Van Tassell <hello@henrikvt.com> (https://henrikvt.com)",
  "license": "MIT",
  "homepage": "https://github.com/henrikvtcodes/jscfg#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/henrikvtcodes/jscfg.git",
    "directory": "core"
  },
  "type": "commonjs",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./eslint": {
      "import": "./dist/eslint.mjs",
      "require": "./dist/eslint.js",
      "types": "./dist/eslint.d.ts"
    },
    "./prettier": {
      "import": "./dist/prettier.mjs",
      "require": "./dist/prettier.js",
      "types": "./dist/prettier.d.ts"
    },
    "./ts/*": "./tsconfigs/*"
  },
  "scripts": {
    "build": "NODE_ENV=production tsup",
    "dev": "tsup --watch"
  },
  "devDependencies": {
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.12.7",
    "tsup": "^8.0.2"
  },
  "peerDependencies": {
    "eslint": "^9.1.1",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@eslint/js": "^9.1.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@typescript-eslint/utils": "^7.7.1",
    "eslint-config-next": "^14.1.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "^1.13.2",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "globals": "^15.0.0",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "typescript-eslint": "^7.7.1"
  },
  "keywords": [
    "javascript",
    "typescript",
    "ecmascript",
    "eslint",
    "eslint-config",
    "lint",
    "config",
    "configuration",
    "prettier",
    "prettier-config",
    "stylelint"
  ]
}