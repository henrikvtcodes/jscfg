{
  "name": "tslnf",
  "version": "0.0.1",
  "description": "All-in-one configuration source for JavaScript/TypeScript projects",
  "author": "Henrik Van Tassell <hello@henrikvt.com> (https://henrikvt.com)",
  "license": "MIT",
  "homepage": "https://github.com/henrikvtcodes/jscfg#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/henrikvtcodes/jscfg.git",
    "directory": "core"
  },
  "type": "commonjs",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./eslint": {
      "import": "./dist/eslint.mjs",
      "require": "./dist/eslint.js",
      "types": "./dist/eslint.d.ts"
    },
    "./prettier": {
      "import": "./dist/prettier.mjs",
      "require": "./dist/prettier.js",
      "types": "./dist/prettier.d.ts"
    },
    "./ts/*": "./tsconfigs/*"
  },
  "scripts": {
    "build": "NODE_ENV=production tsup",
    "dev": "tsup --watch",
    "test": "vitest"
  },
  "dependencies": {
    "@eslint/js": "^9.4.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@next/eslint-plugin-next": "^14.2.3",
    "@typescript-eslint/utils": "8.0.0-alpha.24",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-turbo": "^1.13.3",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-tailwindcss": "^3.17.0",
    "globals": "^15.3.0",
    "prettier-plugin-tailwindcss": "^0.6.1",
    "typescript-eslint": "8.0.0-alpha.24"
  },
  "devDependencies": {
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.13.0",
    "tsup": "^8.0.2",
    "vitest": "^1.6.0"
  },
  "peerDependencies": {
    "eslint": ">=9.4.0",
    "prettier": ">=3.2.5",
    "typescript": ">=5.0.0"
  },
  "keywords": [
    "javascript",
    "typescript",
    "ecmascript",
    "eslint",
    "eslint-config",
    "lint",
    "config",
    "configuration",
    "prettier",
    "prettier-config",
    "stylelint"
  ]
}